{
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/css"
    },
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/css"
    }
  ],
  "liveSassCompile.settings.generateMap": true,
  "liveSassCompile.settings.autoprefix": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "liveSassCompile.settings.includeItems": ["scss/styles.scss"],
  "liveSassCompile.settings.savePathRelativeToProject": true
}
